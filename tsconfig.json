{
  "compilerOptions": {
    "lib": [
      "dom",
      "es6"
    ],
    "target": "ES2017",
    "module": "ES6",
    "moduleResolution": "Node",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true
  }
}